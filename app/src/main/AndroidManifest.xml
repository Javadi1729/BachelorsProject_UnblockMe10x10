                android:name=".ActivityHelpGeneral"
                android:screenOrientation="portrait"
                android:exported="false"/>
        <activity
                android:name=".ActivityGameScene"
                android:screenOrientation="portrait"
                android:exported="false"/>
        <activity
                android:name=".ActivityLevels"
                android:screenOrientation="portrait"
                android:exported="false"/>
        <activity
                android:name=".ActivityHelp"
                android:screenOrientation="portrait"
                android:exported="false"/>
        <activity
                android:name=".ActivityHome"
                android:screenOrientation="portrait"
                android:exported="false"/>
        <activity
                android:name=".ActivitySplashScreen"
                android:screenOrientation="portrait"
                android:exported="true">
